{
  "name": "@beerose/semantic-search",
  "license": "MIT",
  "version": "1.0.0-beta.6",
  "description": "An OpenAI-powered CLI to build a semantic search index from your MDX files.",
  "author": {
    "email": "alexsandra.sikora@gmail.com",
    "name": "Aleksandra Sikora",
    "url": "https://aleksandra.codes"
  },
  "homepage": "https://github.com/beerose/semantic-search/blob/main/README.md",
  "bin": {
    "@beerose/semantic-search": "bin/cli.js"
  },
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "semantic",
    "search",
    "cli",
    "mdx",
    "pinecone",
    "openai"
  ],
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/bin/cli.ts --format cjs,esm --dts --treeshake --minify",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write src,tests"
  },
  "devDependencies": {
    "@edgeandnode/eslint-config": "^1.3.0",
    "@types/mdast": "^3.0.10",
    "@types/node": "^18.13.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "prettier": "^2.8.4",
    "tsup": "^6.6.3",
    "typescript": "^5.0.0-beta",
    "vitest": "^0.28.5"
  },
  "dependencies": {
    "@clack/core": "^0.1.7",
    "@clack/prompts": "^0.4.3",
    "cac": "^6.7.14",
    "dotenv": "^16.0.3",
    "gpt3-tokenizer": "^1.1.5",
    "openai": "^3.1.0",
    "picocolors": "^1.0.0",
    "pinecone-client": "^1.0.1",
    "remark": "^14.0.2",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-mdx": "^2.3.0",
    "remark-smartypants": "^2.0.0",
    "tiny-glob": "^0.2.9",
    "unified": "^10.1.2"
  }
}
